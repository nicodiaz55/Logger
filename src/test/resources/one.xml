<?xml version="1.0" encoding="UTF-8"?>
<Loggers>
<levels>TRACE,DEBUG,INFO,WARNING,ERROR,FATAL</levels>
<logger>
	<name>Logger</name>
	<level>INFO</level>
	<filter>REGEX1</filter>
	<outputs>
		<output>
			<type>console</type>
			<formatter>%d{HH:mm:ss} - %p - %t - %m</formatter>
			<separator>-</separator>
		</output>
	</outputs>
</logger>
</Loggers>